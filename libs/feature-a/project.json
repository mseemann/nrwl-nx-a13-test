{
  "projectType": "library",
  "sourceRoot": "libs/feature-a/src",
  "prefix": "nrwl-nx-a13-test",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/feature-a"],
      "options": {
        "jestConfig": "libs/feature-a/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/feature-a/**/*.ts",
          "libs/feature-a/**/*.html"
        ]
      }
    }
  },
  "tags": [
    "scope:feature"
  ]
}
