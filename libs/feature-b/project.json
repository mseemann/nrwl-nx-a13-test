{
  "projectType": "library",
  "sourceRoot": "libs/feature-b/src",
  "prefix": "nrwl-nx-a13-test",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/feature-b"],
      "options": {
        "jestConfig": "libs/feature-b/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/feature-b/**/*.ts",
          "libs/feature-b/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:feature"]
}
